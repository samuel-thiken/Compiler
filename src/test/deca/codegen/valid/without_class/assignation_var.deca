// Description:
//    Teste tous les types d'assignations de variables
//
// Resultats:
//    my_other_int : 405845 / a : 3.00000e+00 / c : 2.00000e+00
//
// Historique:
//    cree le 07/01/2023
{
    int my_int = 405845;
    int my_other_int = 2;

    float a =  2.0f;
    float b = 3.0;
    float c = a;
    
    my_other_int = my_int;
    a = b;

    print("my_other_int : ", my_other_int);
    print(" / ");
    print("a : ", a);
    print(" / ");
    print("c : ", c);
}