class Point2D {
    int x;
    int y;
    void diag(int a) {
        x = x + a;
        y = y + a;
    }
}

class Point3D extends Point2D {
    int z;
    void diag(int a) {
        x = x + a;
        y = y + a;
        z = z + a;
    }
}

{
    Point2D p1, p2;
    Point3D p3;

    p1 = new Point2D();
    p1.diag(1);

    p3 = new Point3D();
    p3.diag(2);

    p2 = p3;
    p2.diag(3);
    println("p3.z = ", p3.z);
}